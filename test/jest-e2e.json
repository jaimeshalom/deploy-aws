{
  "coverageDirectory": "./.coverage-e2e",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "roots": [
    "<rootDir>/src",
    "<rootDir>/test"
  ],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "collectCoverageFrom": [
    "src/**/*.(t|j)s",
    "!**/node_modules/**",
    "!**/vendor/**",
    "!src/**/*.spec.ts"
  ]
}